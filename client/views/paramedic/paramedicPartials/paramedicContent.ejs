<section id="paramedic-content">
  <div class="container">
    <div class="row justify-content-end">
      <div class="col-12 col-sm-12 col-md-6 form-container">
      <p class="title">Paramedic Training Program</p>
      <p class="disc">In collaboration with AIIMS</p>
      <p class="date">25th Dec, Hauz Khas</p>
<form method='post' action='/paramedic'>
  <div class="form-group">
    <input type="text" class="form-control" name='paramedic_name' id="" placeholder="Name">
  </div>
  <div class="form-group">
    <input type="number" class="form-control" name='paramedic_age' id="" placeholder="Age">
  </div>
  <div class="form-group">
    <input type="tel" class="form-control" name='paramedic_phone' id="" placeholder="Contact No.">
  </div>
  <div class="form-group sex-group">
  <label class="sex">Sex</label>
    <div class="form-check form-check-inline">
  <label class="form-check-label">
    <input class="form-check-input" type="radio"  name="sex" id="inlineRadio1" value="male">Male
  </label>
</div>
<div class="form-check form-check-inline">
  <label class="form-check-label">
    <input class="form-check-input" type="radio" name="sex" id="inlineRadio2" value="female">Female
  </label>
</div>
<div class="form-check form-check-inline">
  <label class="form-check-label">
    <input class="form-check-input" type="radio" name="sex" id="inlineRadio3" value="other" > Other
  </label>
</div>
  </div>
  <div class="form-group">
    <input type="text" class="form-control" id="" name='paramedic_occupation' placeholder="Occupation">
  </div>
  <p class="intermediate-text">Any prior Experience ?</p>
  <div class="form-group">
    <div class="form-check form-check-inline">
      <label class="form-check-label">
      <input class="form-check-input" type="radio" name="paramedic_experience" id="exampleRadios1" value="yes" checked>
      Yes
      </label>
    </div>
    <div class="form-check form-check-inline">
  <label class="form-check-label">
    <input class="form-check-input" type="radio" name="paramedic_experience" id="exampleRadios2" value="no">
    No
  </label>
    </div>
    <div class="form-check form-check-inline">
  <label class="form-check-label">
    <input class="form-control" type="text" name="paramedic_details" id="exampleRadios2" placeholder="If Yes, give details">
  </label>
    </div>
    </div>
  <button type="submit" class="btn  register-btn-invert">Register</button>
</form>
      </div>
    </div>
  </div>
</section>
